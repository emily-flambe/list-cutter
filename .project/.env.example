# Environment Variables for Cutty Project
# Copy this to .env and fill in your values

# API Keys (if using external AI services)
GOOGLE_API_KEY=your-google-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Project Settings
PROJECT_ENV=development
DEBUG_MODE=false

# Cloudflare Workers Development
JWT_SECRET=dev-secret-key-for-local-testing-only
API_KEY_SALT=dev-salt-for-api-keys-local-testing-only

# Google OAuth (Development)
GOOGLE_CLIENT_ID=your-dev-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-dev-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8787/api/v1/auth/google/callback

# Tool-specific settings
GEMINI_SANDBOX=false
CLAUDE_ARTIFACTS=true

# Feature Flags
ENABLE_OAUTH=true
ENABLE_FILE_UPLOADS=true
MAINTENANCE_MODE=false