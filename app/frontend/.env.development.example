# Development environment variables example
# Copy this file to .env.development and update with your actual values

# Environment mode - keep as development for local testing
VITE_ENVIRONMENT=development

# API base URL - default local development server
VITE_API_BASE_URL=http://127.0.0.1:8788

# Maximum upload size in bytes (10MB default)
VITE_MAX_UPLOAD_SIZE=10485760

# Enable debug mode for development
VITE_DEBUG=true

# AI Worker API Key - Replace with your actual API key
# Generate from the dashboard or via API
VITE_AI_WORKER_API_KEY=your-ai-worker-api-key-here

# Agent functionality settings
# Enable/disable the AI agent feature
VITE_AGENT_ENABLED=true

# URL for the Cutty agent service
# Replace with your own worker URL if deploying your own instance
VITE_AGENT_URL=https://cutty-agent.emily-cogsdill.workers.dev